<?xml version="1.0" encoding="utf-8"?>
<system>
	<!-- 产品代号 -->
	<productcode>qxs</productcode>
	<!-- 是否测试模式，测试模式不需要设置ds_info，所有数据库链接都基于db.properties -->
	<istest>false</istest>
	<!-- 是否使用本地内存 -->
	<islocal>false</islocal>
	<isdebug>false</isdebug>
	<!-- standalone(默认单机版,为了开发方便） sentinel(哨兵,线上高可用版) -->
	<mc_type>redis</mc_type>
	<mc_redis_pool_type>standalone</mc_redis_pool_type>
	<mc_redis_pool_addr>hd137:6379</mc_redis_pool_addr>
	<mc_redis_pool_pass>qiuxs</mc_redis_pool_pass>
	<!-- 集群 -->
	<cluster>album01</cluster>
</system>